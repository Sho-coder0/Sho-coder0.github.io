          "scale": "y",
          "maxExtent": 0,
          "ticks": false,
          "grid": true,
          "minExtent": 0,
          "zindex": 0,
          "domain": false,
          "labels": false,
          "orient": "left",
          "tickCount": {"signal": "ceil(child_height/40)"},
          "gridScale": "x",
          "aria": false
        }
      ],
      "title": {
        "text": {
          "signal": "isValid(parent[\"region_s\"]) ? parent[\"region_s\"] : \"\"+parent[\"region_s\"]"
        },
        "style": "guide-label",
        "frame": "group",
        "offset": 10
      },
      "style": "cell"
    }
