      "name": "cell",
      "from": {
        "facet": {"data": "source_0", "name": "facet", "groupby": ["region_s"]}
      },
      "sort": {"field": ["datum[\"region_s\"]"], "order": ["ascending"]},
      "encode": {
        "update": {
          "width": {"signal": "child_width"},
          "height": {"signal": "child_height"}
        }
      },
      "marks": [
        {
          "type": "rect",
          "name": "child_marks",
          "from": {"data": "facet"},
          "encode": {
            "update": {
              "width": {"signal": "max(0.25, bandwidth('x'))"},
              "fill": {"field": "price_type", "scale": "color"},
              "x": {"field": "price_type", "scale": "x"},
              "y": {"field": "price_end", "scale": "y"},
              "y2": {"field": "price_start", "scale": "y"}
            }
          },
          "style": ["bar"]
        }
      ],
